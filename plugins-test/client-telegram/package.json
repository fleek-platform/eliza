{
   "name": "@elizaos-plugins/client-telegram",
   "version": "0.25.6-alpha.1",
   "type": "module",
   "main": "dist/index.js",
   "module": "dist/index.js",
   "types": "dist/index.d.ts",
   "exports": {
      "./package.json": "./package.json",
      ".": {
         "import": {
            "@elizaos/source": "./src/index.ts",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
         }
      }
   },
   "files": [
      "dist"
   ],
   "dependencies": {
      "@elizaos/core": "0.25.9",
      "@telegraf/types": "7.1.0",
      "telegraf": "4.16.3"
   },
   "devDependencies": {
      "@elizaos/core": "0.25.9",
      "@telegraf/types": "7.1.0",
      "telegraf": "4.16.3",
      "tsup": "8.3.5",
      "vitest": "1.6.1"
   },
   "scripts": {
      "build": "tsup --format esm --dts",
      "dev": "tsup --format esm --dts --watch",
      "test": "vitest run",
      "test:watch": "vitest"
   },
   "publishConfig": {
      "access": "public"
   },
   "agentConfig": {
      "pluginType": "elizaos:client:1.0.0",
      "pluginParameters": {
         "TELEGRAM_BOT_TOKEN": {
            "type": "string"
         }
      }
   }
}
