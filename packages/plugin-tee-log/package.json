{
  "name": "@elizaos-plugins/plugin-tee-log",
  "version": "0.1.9",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@elizaos-plugins/plugin-sgx": "workspace:*",
    "@elizaos-plugins/plugin-tee": "workspace:*",
    "@elizaos/core": "workspace:^",
    "better-sqlite3": "11.6.0",
    "elliptic": "6.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20.0.0",
    "tsup": "^8.3.5"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --apply src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "ENABLE_TEE_LOG": {
        "type": "boolean",
        "description": "Enable/disable TEE logging functionality"
      },
      "SGX": {
        "type": "boolean",
        "description": "Enable/disable SGX mode"
      },
      "TEE_MODE": {
        "type": "string",
        "enum": [
          "sgx",
          "nitro",
          "disabled"
        ],
        "description": "Trusted Execution Environment mode"
      },
      "WALLET_SECRET_SALT": {
        "type": "string",
        "minLength": 1,
        "description": "Salt for wallet secret generation"
      }
    }
  }
}
