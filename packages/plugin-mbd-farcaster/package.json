{
  "name": "@elizaos-plugins/plugin-mbd-farcaster",
  "version": "0.1.0",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "MBD_API_KEY": {
        "type": "string",
        "description": "API key for MBD (Mind Blockchain Data)",
        "optional": true
      },
      "MBD_APP_NAME": {
        "type": "string",
        "description": "Application name for MBD API identification",
        "optional": true,
        "default": "eliza_mbd_plugin"
      },
      "MBD_APP_URL": {
        "type": "string",
        "description": "Application URL for MBD API identification",
        "optional": true,
        "default": "https://docs.mbd.xyz/"
      },
      "MBD_DEBUG": {
        "type": "boolean",
        "description": "Enable debug mode for detailed logging",
        "optional": true,
        "default": false
      }
    }
  },
  "dependencies": {
    "@elizaos/core": "^0.25.8"
  }
}