{
  "name": "@elizaos-plugins/plugin-quai",
  "version": "0.1.9",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@avnu/avnu-sdk": "^2.1.1",
    "@elizaos-plugins/plugin-trustdb": "workspace:*",
    "@elizaos/core": "workspace:^",
    "quais": "1.0.0-alpha.25",
    "tsup": "^8.3.5",
    "vitest": "^3.0.0"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "test": "vitest",
    "lint": "eslint --fix  --cache ."
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "QUAI_PRIVATE_KEY": {
        "type": "string",
        "description": "Your Quai network private key"
      },
      "QUAI_RPC_URL": {
        "type": "string",
        "description": "Quai network RPC endpoint (e.g. https://rpc.quai.network)"
      }
    }
  }
}
