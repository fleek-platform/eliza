{
  "name": "@elizaos-plugins/plugin-omniflix",
  "version": "0.1.9",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@coral-xyz/anchor": "0.30.1",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/encoding": "^0.32.4",
    "@elizaos/core": "workspace:*",
    "bs58": "6.0.0",
    "bech32": "2.0.0",
    "cosmjs-types": "^0.8.0",
    "@omniflixnetwork/omniflixjs": "^0.1.1",
    "node-cache": "5.1.2",
    "tsup": "8.3.5",
    "uuid": "^10.0.0",
    "vitest": "2.1.4",
    "@elizaos-plugins/plugin-tee": "workspace:*",
    "@elizaos-plugins/plugin-trustdb": "workspace:*"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "lint": "eslint --fix  --cache .",
    "test": "vitest run"
  },
  "peerDependencies": {
    "form-data": "4.0.1",
    "whatwg-url": "7.1.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "OMNIFLIX_RPC_ENDPOINT": {
        "type": "string",
        "minLength": 1,
        "description": "RPC endpoint for OmniFlix Network",
        "optional": false
      },
      "OMNIFLIX_API_URL": {
        "type": "string",
        "minLength": 1,
        "description": "API endpoint for OmniFlix Network",
        "optional": false
      },
      "OMNIFLIX_MNEMONIC": {
        "type": "string",
        "minLength": 1,
        "description": "Mnemonic for accessing the blockchain. Either mnemonic or private key is required.",
        "optional": true
      },
      "OMNIFLIX_PRIVATE_KEY": {
        "type": "string",
        "minLength": 1,
        "description": "Hex private key for accessing the blockchain. Either mnemonic or private key is required.",
        "optional": true
      }
    }
  }
}