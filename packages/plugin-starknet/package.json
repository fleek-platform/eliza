{
  "name": "@elizaos-plugins/plugin-starknet",
  "version": "0.1.9",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@avnu/avnu-sdk": "2.1.1",
    "@elizaos-plugins/plugin-trustdb": "workspace:*",
    "@elizaos/core": "workspace:^",
    "@uniswap/sdk-core": "6.0.0",
    "@unruggable_starknet/core": "0.1.0",
    "starknet": "6.18.0",
    "tsup": "8.3.5",
    "unruggable-sdk": "1.4.0",
    "vitest": "2.1.9"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check src/",
    "lint:fix": "biome check --apply src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "STARKNET_ADDRESS": {
        "type": "string",
        "minLength": 1,
        "description": "Starknet address is required"
      },
      "STARKNET_PRIVATE_KEY": {
        "type": "string",
        "minLength": 1,
        "description": "Starknet private key is required"
      },
      "STARKNET_RPC_URL": {
        "type": "string",
        "minLength": 1,
        "description": "Starknet RPC URL is required"
      }
    }
  }
}
