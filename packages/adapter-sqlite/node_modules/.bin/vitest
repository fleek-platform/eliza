#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mohit/Desktop/MYPERSONAL/Eliza-OpenSource-Contribution-Repo/eliza/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@types+node@22.13.5_jiti@1.21.7_jsdom@25.0.1_bufferutil@4.0._kgg6hftxvf56njvsj2k3qukrwm/node_modules/vitest/node_modules:/Users/mohit/Desktop/MYPERSONAL/Eliza-OpenSource-Contribution-Repo/eliza/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@types+node@22.13.5_jiti@1.21.7_jsdom@25.0.1_bufferutil@4.0._kgg6hftxvf56njvsj2k3qukrwm/node_modules:/Users/mohit/Desktop/MYPERSONAL/Eliza-OpenSource-Contribution-Repo/eliza/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mohit/Desktop/MYPERSONAL/Eliza-OpenSource-Contribution-Repo/eliza/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@types+node@22.13.5_jiti@1.21.7_jsdom@25.0.1_bufferutil@4.0._kgg6hftxvf56njvsj2k3qukrwm/node_modules/vitest/node_modules:/Users/mohit/Desktop/MYPERSONAL/Eliza-OpenSource-Contribution-Repo/eliza/node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@types+node@22.13.5_jiti@1.21.7_jsdom@25.0.1_bufferutil@4.0._kgg6hftxvf56njvsj2k3qukrwm/node_modules:/Users/mohit/Desktop/MYPERSONAL/Eliza-OpenSource-Contribution-Repo/eliza/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@types+node@22.13.5_jiti@1.21.7_jsdom@25.0.1_bufferutil@4.0._kgg6hftxvf56njvsj2k3qukrwm/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vitest@3.0.5_@types+debug@4.1.12_@types+node@22.13.5_jiti@1.21.7_jsdom@25.0.1_bufferutil@4.0._kgg6hftxvf56njvsj2k3qukrwm/node_modules/vitest/vitest.mjs" "$@"
fi
