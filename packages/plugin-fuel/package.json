{
    "name": "@elizaos-plugins/plugin-fuel",
    "version": "0.1.9",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "workspace:^",
        "fuels": "0.97.2",
        "tsup": "8.3.5",
        "vitest": "2.1.9"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "test": "vitest run",
        "lint": "biome lint .",
        "lint:fix": "biome check --apply .",
        "format": "biome format .",
        "format:fix": "biome format --write ."
    },
    "peerDependencies": {
        "form-data": "4.0.1",
        "whatwg-url": "7.1.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "FUEL_PRIVATE_KEY": {
                "type": "string",
                "description": "Private key for the Fuel wallet starting with 0x"
            },
            "FUEL_PROVIDER_URL": {
                "type": "string",
                "optional": true,
                "default": "https://mainnet.fuel.network/v1/graphql",
                "description": "Custom RPC endpoint URL"
            }
        }
    }
}