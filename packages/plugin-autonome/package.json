{
  "name": "@elizaos-plugins/plugin-autonome",
  "version": "0.1.9",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@coral-xyz/anchor": "0.30.1",
    "@elizaos-plugins/plugin-tee": "workspace:*",
    "@elizaos-plugins/plugin-trustdb": "workspace:*",
    "@elizaos/core": "workspace:^",
    "axios": "^1.7.9",
    "tsup": "8.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "test": "vitest run",
    "lint": "biome lint .",
    "lint:fix": "biome check --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write ."
  },
  "peerDependencies": {
    "form-data": "4.0.1",
    "whatwg-url": "7.1.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "AUTONOME_JWT_TOKEN": {
        "type": "string"
      },
      "AUTONOME_RPC": {
        "type": "string"
      }
    }
  }
}
