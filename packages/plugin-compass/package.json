{
  "name": "@elizaos-plugins/plugin-compass",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "dev": "tsup --format esm --dts --watch"
  },
  "author": "royalnine",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^1.2.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@compass-labs/sdk": "^1.0.8",
    "@elizaos/core": "workspace:*",
    "viem": "2.21.58",
    "zod": "^3.24.2"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "COMPASS_WALLET_PRIVATE_KEY": {
        "type": "string",
        "description": "EVM wallet private key"
      },
      "COMPASS_ARBITRUM_RPC_URL": {
        "type": "string",
        "description": "Arbitrum mainnet RPC URL"
      },
      "COMPASS_ETHEREUM_RPC_URL": {
        "type": "string",
        "description": "Ethereum mainnet RPC URL"
      },
      "COMPASS_BASE_RPC_URL": {
        "type": "string",
        "description": "Base mainnet RPC URL"
      }
    }
  }
}
