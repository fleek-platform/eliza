{
    "name": "@elizaos-plugins/plugin-tee-marlin",
    "version": "0.1.9",
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@elizaos/core": "workspace:^",
        "tsup": "^8.3.5"
    },
    "devDependencies": {
        "@biomejs/biome": "1.5.3"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "biome check src/",
        "lint:fix": "biome check --apply src/",
        "format": "biome format src/",
        "format:fix": "biome format --write src/"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "TEE_MARLIN_ATTESTATION_ENDPOINT": {
                "type": "string",
                "minLength": 1,
                "description": "Endpoint URL for TEE Marlin attestation service"
            }
        }
    }
}