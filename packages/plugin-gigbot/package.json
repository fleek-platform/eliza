{
  "name": "@elizaos-plugins/plugin-gigbot",
  "version": "0.25.8",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/core": "workspace:^",
    "agent-twitter-client": "0.0.18",
    "dotenv": "^16.4.7",
    "glob": "11.0.0",
    "viem": "latest",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "1.1.3",
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "8.3.5",
    "typescript": "^5.7.2",
    "vitest": "1.6.1"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "GIGBOT_API_URL": {
        "type": "string",
        "default": "https://www.gigbot.xyz/api",
        "description": "API URL for Gigbot operations"
      },
      "GIG_SEARCH_INTERVAL": {
        "type": "string",
        "default": "3",
        "description": "Interval for searching gigs (in hours)"
      },
      "GIG_ACTION_INTERVAL": {
        "type": "string",
        "default": "12",
        "description": "Interval for performing gig actions (in hours)"
      },
      "GIG_CLAIM_INTERVAL": {
        "type": "string",
        "default": "24",
        "description": "Interval for claiming gigs (in hours)"
      },
      "GIG_CLAIM_PLATFORM": {
        "type": "string",
        "enum": [
          "x",
          "farcaster"
        ],
        "default": "x",
        "description": "Platform for claiming gig rewards"
      }
    }
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
